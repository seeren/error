	"keywords" : [
		"error",
		"handler",
		"logger"
	],
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Cyril Ichti",
			"email" : "consultant@seeren.fr",
		"homepage" : "https://github.com/seeren/error"
		}
	],
	"require" : {
		"php" : ">=7.4.0",
		"seeren/log": "^2.0"
	},
	"autoload" : {
